name: "GoogleNet"
layer {
  name: "data_loading"
  type: "Python"
  top: "image_1"
  top: "image_2"
  top: "correspondence"
  top: "num_coord"
  top: "img_size"
  top: "image_1_azimuth"
  top: "image_2_azimuth"
  python_param {
    module: "lib.layers.pair_data_layer_all_classes"
    layer: "IntraClassCorrespondenceDataLayer"
    param_str: "phase: TRAIN\ndb: kittiflow"
  }
}


layer {
  bottom: "img_size"
  name: "img_size_silence"
  type: "Silence"
}


layer {
  bottom: "image_2_azimuth"
